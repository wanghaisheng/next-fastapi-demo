    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "axios": "^1.6.2",
    "concurrently": "^8.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "next": "^14.0.3",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
